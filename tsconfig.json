{
    "extends": "@vue/tsconfig/tsconfig.web.json",
    "include": [
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue",
        "version.cjs"
    ],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "lib": [
            "DOM",
            "ES2017"
        ],
        "jsx": "preserve"
    },
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}